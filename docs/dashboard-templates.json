{
  "dashboardTemplates": {
    "executive-summary": {
      "name": "Executive Summary Dashboard",
      "description": "High-level KPI overview for leadership and clients",
      "type": "client-facing",
      "frequency": "weekly/monthly",
      "pages": 1,
      "layout": "responsive",
      "purpose": "Quick snapshot of business health and key metrics",
      "targetAudience": ["C-Level", "Clients", "Stakeholders"],
      "pages_config": [
        {
          "pageNumber": 1,
          "title": "Executive Overview",
          "sections": [
            {
              "type": "header",
              "content": "Dashboard Title - [Date Range]",
              "styling": "prominent"
            },
            {
              "type": "kpi_row",
              "metrics": [
                {
                  "name": "Users",
                  "dimension": "User Count",
                  "comparison": "month-over-month",
                  "visualization": "scorecard",
                  "showTrend": true
                },
                {
                  "name": "Sessions",
                  "dimension": "Session Count",
                  "comparison": "month-over-month",
                  "visualization": "scorecard",
                  "showTrend": true
                },
                {
                  "name": "Conversion Rate",
                  "dimension": "Conversions / Sessions",
                  "comparison": "month-over-month",
                  "visualization": "scorecard",
                  "showTrend": true
                },
                {
                  "name": "Revenue",
                  "dimension": "Total Revenue",
                  "comparison": "month-over-month",
                  "visualization": "scorecard",
                  "showTrend": true
                }
              ]
            },
            {
              "type": "chart_row",
              "row": 1,
              "charts": [
                {
                  "title": "Users Trend",
                  "type": "line_chart",
                  "metric": "Users",
                  "dimension": "Date",
                  "timeRange": "last_90_days",
                  "width": "50%"
                },
                {
                  "title": "Traffic by Device",
                  "type": "pie_chart",
                  "metric": "Sessions",
                  "dimension": "Device Category",
                  "width": "50%"
                }
              ]
            },
            {
              "type": "chart_row",
              "row": 2,
              "charts": [
                {
                  "title": "Revenue by Channel",
                  "type": "bar_chart",
                  "metric": "Revenue",
                  "dimension": "Source / Medium",
                  "sortBy": "Revenue",
                  "sortOrder": "descending",
                  "limit": 5,
                  "width": "50%"
                },
                {
                  "title": "Conversion by Source",
                  "type": "table",
                  "columns": [
                    "Source / Medium",
                    "Users",
                    "Conversions",
                    "Conversion Rate"
                  ],
                  "limit": 10,
                  "width": "50%"
                }
              ]
            },
            {
              "type": "text_box",
              "title": "Key Insights",
              "content": "[Insert key findings and highlights for this period]",
              "styling": "highlight"
            },
            {
              "type": "text_box",
              "title": "Recommendations",
              "content": "[Insert actionable recommendations based on data]",
              "styling": "action"
            }
          ],
          "filters": [
            {
              "name": "Date Range",
              "type": "date_range",
              "position": "top",
              "default": "last_month"
            }
          ]
        }
      ]
    },
    "performance-dashboard-client": {
      "name": "Performance Dashboard (Client-Facing)",
      "description": "Comprehensive monthly client reporting with insights",
      "type": "client-facing",
      "frequency": "weekly/monthly",
      "pages": 4,
      "layout": "responsive",
      "purpose": "Detailed performance review with actionable insights",
      "targetAudience": ["Clients", "Account Managers", "Stakeholders"],
      "pages_config": [
        {
          "pageNumber": 1,
          "title": "Monthly Overview",
          "sections": [
            {
              "type": "header",
              "content": "Performance Report - [Client Name] - [Month/Period]"
            },
            {
              "type": "kpi_row",
              "metrics": [
                {
                  "name": "Users",
                  "comparison": "month-over-month",
                  "visualization": "scorecard",
                  "showTrend": true
                },
                {
                  "name": "Sessions",
                  "comparison": "month-over-month",
                  "visualization": "scorecard",
                  "showTrend": true
                },
                {
                  "name": "Conversions",
                  "comparison": "month-over-month",
                  "visualization": "scorecard",
                  "showTrend": true
                },
                {
                  "name": "Revenue",
                  "comparison": "month-over-month",
                  "visualization": "scorecard",
                  "showTrend": true
                }
              ]
            },
            {
              "type": "text_box",
              "title": "Performance Highlight",
              "content": "[Insert key achievement or win for the period]"
            },
            {
              "type": "chart",
              "title": "Users & Sessions Trend",
              "type": "dual_axis_chart",
              "metrics": ["Users", "Sessions"],
              "dimension": "Date"
            }
          ]
        },
        {
          "pageNumber": 2,
          "title": "Traffic Analysis",
          "sections": [
            {
              "type": "chart_row",
              "charts": [
                {
                  "title": "Users by Traffic Source",
                  "type": "bar_chart",
                  "metric": "Users",
                  "dimension": "Source / Medium",
                  "width": "50%"
                },
                {
                  "title": "Device Breakdown",
                  "type": "pie_chart",
                  "metric": "Sessions",
                  "dimension": "Device Category",
                  "width": "50%"
                }
              ]
            },
            {
              "type": "chart",
              "title": "Traffic Trends",
              "type": "line_chart",
              "metric": "Users",
              "dimension": "Date"
            },
            {
              "type": "table",
              "title": "Top Landing Pages",
              "columns": [
                "Page Title",
                "Users",
                "Sessions",
                "Bounce Rate",
                "Avg Duration"
              ],
              "limit": 10
            }
          ],
          "filters": [
            {
              "name": "Date Range",
              "type": "date_range",
              "default": "last_month"
            },
            {
              "name": "Traffic Source",
              "type": "dropdown",
              "dimension": "Source / Medium"
            }
          ]
        },
        {
          "pageNumber": 3,
          "title": "Conversions & Revenue",
          "sections": [
            {
              "type": "kpi_row",
              "metrics": [
                {
                  "name": "Conversion Rate",
                  "visualization": "scorecard",
                  "comparison": "month-over-month"
                },
                {
                  "name": "Revenue",
                  "visualization": "scorecard",
                  "comparison": "month-over-month"
                },
                {
                  "name": "ROAS",
                  "visualization": "scorecard",
                  "comparison": "month-over-month"
                },
                {
                  "name": "Cost Per Conversion",
                  "visualization": "scorecard",
                  "comparison": "month-over-month"
                }
              ]
            },
            {
              "type": "chart_row",
              "charts": [
                {
                  "title": "Conversion Rate Trend",
                  "type": "line_chart",
                  "metric": "Conversion Rate",
                  "dimension": "Date",
                  "width": "50%"
                },
                {
                  "title": "Revenue by Source",
                  "type": "bar_chart",
                  "metric": "Revenue",
                  "dimension": "Source / Medium",
                  "width": "50%"
                }
              ]
            },
            {
              "type": "table",
              "title": "Conversion by Traffic Source",
              "columns": [
                "Source / Medium",
                "Users",
                "Conversions",
                "Conversion Rate",
                "Revenue",
                "ROAS"
              ]
            }
          ]
        },
        {
          "pageNumber": 4,
          "title": "Insights & Recommendations",
          "sections": [
            {
              "type": "text_box",
              "title": "What's Working",
              "content": "[List top 3-5 performing areas and why they're working well]"
            },
            {
              "type": "text_box",
              "title": "Areas to Improve",
              "content": "[List 2-3 areas that need attention and recommendations]"
            },
            {
              "type": "text_box",
              "title": "Recommended Next Steps",
              "content": "[Provide 3-5 specific actionable recommendations for next month]"
            },
            {
              "type": "text_box",
              "title": "Questions & Notes",
              "content": "[Space for questions or additional context]"
            }
          ]
        }
      ]
    },
    "agency-performance": {
      "name": "Agency Performance Dashboard (Internal)",
      "description": "Multi-client analytics for agency teams",
      "type": "internal",
      "frequency": "daily/weekly",
      "pages": 5,
      "layout": "freeform",
      "purpose": "Track performance across all clients and campaigns",
      "targetAudience": ["Agency Team", "Account Managers", "Strategists"],
      "pages_config": [
        {
          "pageNumber": 1,
          "title": "Client Portfolio Overview",
          "sections": [
            {
              "type": "kpi_row",
              "metrics": [
                {
                  "name": "Active Clients",
                  "metric": "Count of Unique Accounts"
                },
                {
                  "name": "Total Managed Budget",
                  "metric": "Sum of Ad Spend"
                },
                {
                  "name": "Average ROAS",
                  "metric": "Average Revenue / Cost"
                },
                {
                  "name": "Active Campaigns",
                  "metric": "Count of Active Campaigns"
                }
              ]
            },
            {
              "type": "table",
              "title": "Client Performance Matrix",
              "columns": [
                "Client Name",
                "Revenue",
                "Cost",
                "ROAS",
                "Conversions",
                "Status"
              ],
              "sortBy": "Revenue",
              "sortOrder": "descending"
            }
          ]
        },
        {
          "pageNumber": 2,
          "title": "Aggregate Performance",
          "sections": [
            {
              "type": "chart_row",
              "charts": [
                {
                  "title": "Combined Users Trend",
                  "type": "line_chart",
                  "metric": "Users",
                  "dimension": "Date",
                  "width": "50%"
                },
                {
                  "title": "Combined Revenue Trend",
                  "type": "line_chart",
                  "metric": "Revenue",
                  "dimension": "Date",
                  "width": "50%"
                }
              ]
            },
            {
              "type": "chart_row",
              "charts": [
                {
                  "title": "ROAS by Client",
                  "type": "bar_chart",
                  "metric": "ROAS",
                  "dimension": "Client Name",
                  "sortBy": "ROAS",
                  "sortOrder": "descending",
                  "width": "50%"
                },
                {
                  "title": "Top Performing Campaigns",
                  "type": "scorecard",
                  "metric": "Campaign with Highest ROAS",
                  "width": "50%"
                }
              ]
            }
          ]
        },
        {
          "pageNumber": 3,
          "title": "Budget Allocation",
          "sections": [
            {
              "type": "chart_row",
              "charts": [
                {
                  "title": "Budget Distribution by Client",
                  "type": "pie_chart",
                  "metric": "Cost",
                  "dimension": "Client Name",
                  "width": "50%"
                },
                {
                  "title": "Budget Trends",
                  "type": "line_chart",
                  "metric": "Cost",
                  "dimension": "Date",
                  "width": "50%"
                }
              ]
            },
            {
              "type": "table",
              "title": "Budget vs Revenue by Client",
              "columns": [
                "Client Name",
                "Budget",
                "Spend",
                "Burn Rate",
                "Revenue",
                "ROI"
              ]
            }
          ]
        },
        {
          "pageNumber": 4,
          "title": "Campaign Deep Dive",
          "sections": [
            {
              "type": "table",
              "title": "All Campaigns Performance",
              "columns": [
                "Campaign Name",
                "Client",
                "Cost",
                "Conversions",
                "Revenue",
                "ROAS",
                "Status"
              ],
              "sortBy": "ROAS",
              "sortOrder": "descending",
              "limit": 50
            }
          ],
          "filters": [
            {
              "name": "Client",
              "type": "dropdown"
            },
            {
              "name": "Campaign Status",
              "type": "dropdown",
              "options": ["Active", "Paused", "Ended"]
            },
            {
              "name": "Date Range",
              "type": "date_range"
            }
          ]
        },
        {
          "pageNumber": 5,
          "title": "Alerts & Actions",
          "sections": [
            {
              "type": "alert_section",
              "title": "Underperforming Campaigns",
              "alertType": "warning",
              "content": "[Table of campaigns with ROAS below threshold]"
            },
            {
              "type": "alert_section",
              "title": "Opportunities",
              "alertType": "success",
              "content": "[Table of high-performing campaigns ready to scale]"
            },
            {
              "type": "text_box",
              "title": "This Week's Action Items",
              "content": "[List of recommended actions and optimizations]"
            }
          ]
        }
      ]
